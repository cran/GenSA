citHeader("To cite GenSA in publications use:")

citEntry(entry = "Article",
  title        = "Generalized Simulated Annealing for Global Optimization: the {GenSA} Package.",
  author       = personList(person("Yang Xiang"),
                   as.person("Sylvain Gubian"),
                   as.person("Brian Suomela"),
                   as.person("Julia Hoeng")),
  journal      = "The R Journal Volume 5(1):13-29, June 2013",
  year         = "2013",
  note         = "",
  url          = "http://journal.r-project.org/archive/2013-1/xiang-gubian-suomela-etal.pdf",
  textVersion  =
  paste("Yang Xiang, Sylvain Gubian, Brian Suomela, Julia Hoeng.",
        "Generalized Simulated Annealing for Global Optimization: the GenSA Package.",
        "The R Journal, Volume 5(1):13-29, June 2013.",
        "URL http://journal.r-project.org/archive/2013-1/xiang-gubian-suomela-etal.pdf")
)
